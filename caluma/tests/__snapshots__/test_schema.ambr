  * 'answer' == 'question-slug'|answer
  * 'answer' in 'list-question-slug'|answer
  * 'answer' in 'table-question-slug'|answer|mapby('column-question')
  * 'answer' in 'table-question-slug'|answer|mapby('multiple-choice-question')|flatten
